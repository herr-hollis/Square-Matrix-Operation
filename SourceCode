    private void jButton2ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton2ActionPerformed
        // TODO add your handling code here:
        double i,j,k,l,m,n,o,p;
        double ans5,ans6,ans7,ans8;
        
        i=Double.parseDouble(textA.getText());
        j=Double.parseDouble(textB.getText());
        k=Double.parseDouble(textC.getText());
        l=Double.parseDouble(textD.getText());
        m=Double.parseDouble(textE.getText());
        n=Double.parseDouble(textF.getText());
        o=Double.parseDouble(textG.getText());
        p=Double.parseDouble(textH.getText());
        
        ans5 = i + m;
        ans6 = j + n;
        ans7 = k + o;
        ans8 = l + p;
        
        System.out.print(ans5);
        System.out.print(ans6);
        System.out.print(ans7);
        System.out.print(ans8);
        
        textI.setText(String.valueOf(ans5));
        textJ.setText(String.valueOf(ans6));
        textK.setText(String.valueOf(ans7));
        textL.setText(String.valueOf(ans8));        
    }//GEN-LAST:event_jButton2ActionPerformed

    private void jButton3ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton3ActionPerformed
        // TODO add your handling code here:
        double q,r,s,t,u,v,w,x;
        double ans9,ans10,ans11,ans12;
        
        q=Double.parseDouble(textA.getText());
        r=Double.parseDouble(textB.getText());
        s=Double.parseDouble(textC.getText());
        t=Double.parseDouble(textD.getText());
        u=Double.parseDouble(textE.getText());
        v=Double.parseDouble(textF.getText());
        w=Double.parseDouble(textG.getText());
        x=Double.parseDouble(textH.getText());
        
        ans9 = q - u;
        ans10 = r - v;
        ans11 = s - w;
        ans12 = t - x;
        
        System.out.print(ans9);
        System.out.print(ans10);
        System.out.print(ans11);
        System.out.print(ans12);
        
        textI.setText(String.valueOf(ans9));
        textJ.setText(String.valueOf(ans10));
        textK.setText(String.valueOf(ans11));
        textL.setText(String.valueOf(ans12));         
    }//GEN-LAST:event_jButton3ActionPerformed

