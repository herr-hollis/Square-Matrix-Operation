    private void jButton1ActionPerformed(java.awt.event.ActionEvent evt) {//GEN-FIRST:event_jButton1ActionPerformed
        // TODO add your handling code here:
        double a,b,c,d,e,f,g,h;
        double ans1,ans2,ans3,ans4;
        
        a=Double.parseDouble(textA.getText());
        b=Double.parseDouble(textB.getText());
        c=Double.parseDouble(textC.getText());
        d=Double.parseDouble(textD.getText());
        e=Double.parseDouble(textE.getText());
        f=Double.parseDouble(textF.getText());
        g=Double.parseDouble(textG.getText());
        h=Double.parseDouble(textH.getText());
        
        ans1 = a*e + c*f;
        ans2 = b*e + d*f;
        ans3 = a*g + c*h;
        ans4 = b*g + d*h;
        
        System.out.print(ans1);
        System.out.print(ans2);
        System.out.print(ans3);
        System.out.print(ans4);
        
        textI.setText(String.valueOf(ans1));
        textJ.setText(String.valueOf(ans2));
        textK.setText(String.valueOf(ans3));
        textL.setText(String.valueOf(ans4));
        
    }//GEN-LAST:event_jButton1ActionPerformed

